<div>
<%= render "shared/nav_dropdown" %>
</div>
<div class="new-shipment-background">
  <div id="new-shipment-cube"><p class="new-shipment-title">BUILD</p></div>
      <div class="new-shipment-inputs">
        <div class="new-shipment-input-inner-layer">
        <h2> LENGTH " </h2>
          <input
            id="inp-length"
            type="range"
            min="1"
            max="33"
            value="16"
            step=".1"
          />
          <div>
            <input
              id="numberL"
              min="1"
              max="33"
              type="number"
              value="16">inches
            </input>
          </div>
        <h2>WIDTH "</h2>
          <input
            id="inp-width"
            type="range"
            min="1"
            max="33"
            value="16"
            step=".1"
            />
          <div>  
          <input
            id="numberW"
            min="1"
            max="33"
            type="number"
            value="16">Inches
          </input>
          </div>
        <h2>HEIGHT "</h2>
          <input
            id="inp-height"
            type="range"
            min="1"
            max="33"
            value="16"
            step=".1"
          />
          <div>
          <input
            id="numberH"
            min="1"
            max="33"
            type="number"
            value="16">Inches
          </input>
          </div>
        <h2>WEIGHT</h2>
          <input
            id="inp-weight"
            min="1"
            max="600"
            type="range"
            value="300"
            step=".1"
          />
          <div>
          <input
            id="numberWeight"
            min="1"
            max="600"
            type="number"
            value="300">Ounces
          </input>
          </div>
          <div>
          <div>
        <%= button_tag("NEXT", id: "next")  %>
       </div> 
  </div>
</div>
      
